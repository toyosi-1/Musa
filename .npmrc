# Ensure consistent package installation
auto-install-peers=true
legacy-peer-deps=true

# Optimize package installation
prefer-offline=true
prefer-frozen-lockfile=true

# Increase network timeout to 10 minutes
timeout=600000

# Disable package-lock.json since we're using npm-shrinkwrap.json
package-lock=false

# Enable strict SSL
strict-ssl=true

# Set registry to default npm registry
registry=https://registry.npmjs.org/
